<%= form_for([@user, @user.expenses.build]) do |f| %>
  <%= f.date_field :entry_date, placeholder: 'date', class: 'date-input form-input' %>
  <%= f.text_field :description, placeholder: 'description', class: 'desc-input form-input' %>
  <%= f.number_field :outflow, :step => 0.01, placeholder: 'enter amount', class: 'amount-input form-input' %>
  <%= f.submit '-' %>
<% end %>